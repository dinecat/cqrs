{
    "name": "dinecat/cqrs",
    "type": "library",
    "homepage": "https://git.dinecat.com/dinecat/cqrs",
    "description": "Provide Command, Event and Query buses and functionality.",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Mykola Zyk",
            "email": "mykola.zyk@dinecat.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/messenger": "^5.2",
        "symfony/validator": "^5.2"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.18",
        "phpstan/phpstan": "^0.12.82",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Dinecat\\Cqrs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dinecat\\CqrsTests\\": "tests"
        }
    }
}
