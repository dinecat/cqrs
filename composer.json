{
    "name": "dinecat/cqrs",
    "type": "library",
    "homepage": "https://git.dinecat.com/dinecat/cqrs",
    "description": "Provide Command, Event and Query buses and functionality.",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Mykola Zyk",
            "email": "mykola.zyk@dinecat.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/messenger": "^5.2|^6.0",
        "symfony/validator": "^5.2|^6.0",
        "symfony/property-access": "^5.2|^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpstan/phpstan": "^1.0.1",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Dinecat\\Cqrs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dinecat\\CqrsTests\\": "tests"
        }
    }
}
